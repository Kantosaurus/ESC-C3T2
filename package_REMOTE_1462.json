{
  "name": "carely",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "main": "./dist/index.mjs",
  "scripts": {
    "start": "node .",
    "dev": "ts-node ./scripts/dev.ts",
    "dev-core": "npm run dev --workspace=@carely/core",
    "dev-backend": "npm run dev --workspace=@carely/backend",
    "dev-frontend": "npm run dev --workspace=@carely/frontend",
    "build": "ts-node ./scripts/build.ts",
    "test": "npm run test:run --workspaces --if-present",
    "coverage": "npm run coverage --workspaces --if-present",
    "lint": "npm run lint --workspaces",
    "prepare": "husky",
    "migrate": "ts-node ./scripts/migrate.ts",
    "clean": "ts-node ./scripts/clean.ts"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/ical": "^0.8.3",
    "@vitest/ui": "^3.2.4",
    "concurrently": "^9.1.2",
    "cypress": "^14.5.4",
    "dotenv": "^17.2.1",
    "eslint": "^9.29.0",
    "husky": "^9.1.7",
    "pg": "^8.16.3",
    "postgres-migrations": "^5.3.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.34.0"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-select": "^2.2.5",
    "@types/qrcode": "^1.5.5",
    "framer-motion": "^12.23.6",
    "ical": "^0.8.0",
    "qrcode": "^1.5.4",
    "react": "^19.1.1",
    "react-day-picker": "^9.8.0",
    "react-dom": "^19.1.1",
    "react-ios-time-picker": "^0.2.2",
    "ts-ics": "^2.2.0"
  }
}
